{
  "collectCoverage": true,
  "collectCoverageFrom": [
      "src/**/*.js",
      "./index.js"
  ],
  "coveragePathIgnorePatterns": [
      "node_modules",
      "tests"
  ],
  "coverageDirectory": "<rootDir>/coverage/",
  "coverageThreshold": {
      "global": {
          "branches": 90,
          "functions": 90,
          "lines": 90,
          "statements": 90
      }
  },
  "verbose": false
}