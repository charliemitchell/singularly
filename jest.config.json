{
  "collectCoverage": true,
  "collectCoverageFrom": [
      "src/**/*.js",
      "./index.js"
  ],
  "coveragePathIgnorePatterns": [
      "node_modules",
      "tests"
  ],
  "coverageDirectory": "<rootDir>/coverage/",
  "coverageThreshold": {
      "global": {
          "branches": 60,
          "functions": 60,
          "lines": 60,
          "statements": 60
      }
  },
  "verbose": false
}